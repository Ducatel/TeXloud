import socket
import json

adresseIP="127.0.0.1"
port=12800
req=dict()


#req['label']="create"
#req['username']="UnNom"
#req['projectName']="projetToto"

s=socket.socket(socket.AF_INET, socket.SOCK_STREAM)
s.connect((adresseIP,port))
s.send(json.dumps(req))
s.close()
